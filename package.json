{
  "name": "react-native-draftjs-render",
  "description": "React Native render for draft.js model",
  "version": "0.2.0",
  "scripts": {
    "test": "jest",
    "test-reset": "npm test -- --updateSnapshot",
    "coverage": "npm test -- --coverage",
    "linter": "eslint .",
    "flow": "flow",
    "flow-stop": "flow stop"
  },
  "main": "index.js",
  "keywords": [
    "react-native",
    "draft-js"
  ],
  "author": "globo.com",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.4.2",
    "react-native": "^0.41.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-react-native": "^2.3.1",
    "flow-bin": "^0.41.0",
    "babel-jest": "19.0.0",
    "jest": "19.0.2",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "/sample/node_modules/"
    ],
    "clearMocks": true,
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/sample/",
      "/coverage/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/globoi/react-native-draftjs-render.git"
  },
  "bugs": {
    "url": "https://github.com/globoi/react-native-draftjs-render/issues"
  },
  "homepage": "https://github.com/globoi/react-native-draftjs-render#readme"
}
